{
    "name": "sunaoka/push-notifications-php",
    "description": "Push notifications/messages for mobile devices",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "sunaoka",
            "email": "sunaoka@pocari.org"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0 || ^8.0",
        "ext-json": "*",
        "ext-openssl": "*",
        "google/apiclient": "^2.8",
        "guzzlehttp/guzzle": "^6.0 || ^7.0",
        "vlucas/valitron": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0 || ^5.0 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "Sunaoka\\PushNotifications\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sunaoka\\PushNotifications\\Tests\\": "./tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "pre-autoload-dump": [
            "Google\\Task\\Composer::cleanup"
        ]
    },
    "extra": {
        "google/apiclient-services": [
            "FirebaseCloudMessaging"
        ]
    }
}
